<%@page contentType="text/html" pageEncoding="UTF-8" isELIgnored="false"%>
<%@include file="/context/mytags.jsp"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">		
<title></title>

</head>
<t:base type="jquery,easyui,tools"></t:base>
<body>
 <t:formvalid formid="formobj"  dialog="true"  layout="table" action="systemset.do?">
<table style="width: 800px;" cellpadding="0" cellspacing="1" class="formtable">
				<tr>
					<td align="right">
						<label class="Validform_label">
							请选择类别(0:定时,1:连续,2:关闭)：
						</label>
					</td>
					<td class="value"   >
					
						<input id="smstype" name="smstype" value="${systemmanager.smstype}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							请选择报警方式(1:即时,2:汇总)：
						</label>
					</td>
					<td class="value"  >
					
						<input id="sendtype" name="sendtype" value="${systemmanager.sendtype}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							汇总盘数:
						</label>
					</td>
					<td class="value"  >
					
						<input id="panshu" name="panshu" value="${systemmanager.panshu}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							上午开始时间:
						</label>
					</td>
					<td class="value"  >
					
						<input id=ambegin name="ambegin" value="${systemmanager.ambegin}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							上午结束时间:
						</label>
					</td>
					<td class="value"  >
					
						<input id=amend name="amend" value="${systemmanager.amend}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							下午开始时间:
						</label>
					</td>
					<td class="value"  >
					
						<input id=pmbegin name="pmbegin" value="${systemmanager.pmbegin}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							下午结束时间:
						</label>
					</td>
					<td class="value"  >
					
						<input id=pmend name="pmend" value="${systemmanager.pmend}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							每盘报警次数:
						</label>
					</td>
					<td class="value"  >
					
						<input id=onefrequency name="onefrequency" value="${systemmanager.onefrequency}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							累计判断盘数:
						</label>
					</td>
					<td class="value"  >
					
						<input id=panduanpanshu name="panduanpanshu" value="${systemmanager.panduanpanshu}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							发送频率:
						</label>
					</td>
					<td class="value"  >
					
						<input id=frequency name="frequency" value="${systemmanager.frequency}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							最后发送时间:
						</label>
					</td>
					<td class="value"  >
					
						<input id=sendtime name="sendtime" value="${systemmanager.sendtime}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							短信发送通道(一般97、123、0):
						</label>
					</td>
					<td class="value"  >
					
						<input id=apitype name="apitype" value="${systemmanager.apitype}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							每盘报警信息格式:
						</label>
					</td>
					<td class="value"  >
					
						<input id=meipan name="meipan" value="${systemmanager.meipan}">
						
					</td>
				</tr>
				<tr>
					<td align="right">
						<label class="Validform_label">
							汇总报警信息格式:
						</label>
					</td>
					<td class="value"  >
					
						<input id=huizong name="huizong" value="${systemmanager.huizong}">
						
					</td>
				</tr>
				<tr>					
					<td colspan="2" align="center">
					
						<input id="tijiao" name="tijiao" type="submit">
						
					</td>
				</tr>
				
			</table>
	</t:formvalid>

</body>

</html>